/dts-v1/;

#include "meson-g12a-x96-max.dtsi"

/ {
	compatible = "amediatech,x96-max-100", "amlogic,u200", "amlogic,g12a";
	model = "Shenzhen Amediatech Technology Co., Ltd X96 Max";
};

&ethmac {
        pinctrl-0 = <&eth_rmii_pins>;
        pinctrl-names = "default";
        status = "okay";
        phy-mode = "rmii";
        phy-handle = <&internal_ephy>;
};
