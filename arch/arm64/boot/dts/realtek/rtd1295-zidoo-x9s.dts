/dts-v1/;

#include <dt-bindings/gpio/gpio.h>

#include "rtd1295.dtsi"

/ {
	compatible = "zidoo,x9s", "realtek,rtd1295";
	model = "Zidoo X9S";

	memory@1f000 {
		device_type = "memory";
		reg = <0x1f000 0x7ffe1000>; /* boot ROM to 2 GiB */
	};

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	/*firmware {
		optee {
			compatible = "linaro,optee-tz";
			method = "smc";
		};
	};*/

	sata0_reg: sata0-regulator {
		compatible = "regulator-fixed";
		regulator-name = "SATA0";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&misc_gpio 16 GPIO_ACTIVE_HIGH>;
		enable-active-high;
	};
};

&cpu0 {
	cpu-supply = <&g2227_dcdc2>;
};

&cpu1 {
	cpu-supply = <&g2227_dcdc2>;
};

&cpu2 {
	cpu-supply = <&g2227_dcdc2>;
};

&cpu3 {
	cpu-supply = <&g2227_dcdc2>;
};

&i2c_0 {
	status = "okay";
	clock-frequency = <400000>;

#include "rtd129x-g2227.dtsi"
};

&irda {
	status = "okay";
};

&iso_gpio {
	hog33 {
		gpio-hog;
		output-low;
		gpios = <33 GPIO_ACTIVE_HIGH>;
	};
};

&l2 {
	l2-supply = <&g2227_ldo3>;
};

&mali {
	mali-supply = <&g2227_dcdc3>;
};

&misc_gpio {
	hog8 {
		gpio-hog;
		input;
		gpios = <8 GPIO_ACTIVE_HIGH>;
	};

	hog100 {
		gpio-hog;
		output-high;
		gpios = <100 GPIO_ACTIVE_HIGH>;
	};
};

&sata {
	status = "okay";
};

&sata0 {
	target-supply = <&sata0_reg>;
};

&uart0 {
	status = "okay";
};

&uart1 {
	status = "okay";
};
